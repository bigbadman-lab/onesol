{
	"cli": {
		"version": ">= 15.0.15",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_BASE_URL": "https://app.1sol.fun",
				"EXPO_PUBLIC_PROXY_BASE_URL": "https://app.1sol.fun"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_BASE_URL": "https://app.1sol.fun",
				"EXPO_PUBLIC_PROXY_BASE_URL": "https://app.1sol.fun"
			}
		},
		"production": {
			"autoIncrement": true,
			"env": {
				"EXPO_PUBLIC_BASE_URL": "https://app.1sol.fun",
				"EXPO_PUBLIC_PROXY_BASE_URL": "https://app.1sol.fun"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
